<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="github/Adara"
          fetch="https://github.com/Project-Adara"
          revision="refs/heads/R" />
  
  <remote name="github/Adara-Devices"
          fetch="https://github.com/Project-Adara-Devices"
          revision="refs/heads/R" />

  <!-- Build -->
  <project path="build/make" name="android_build" groups="pdk" remote="github/Adara" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/blueprint" name="android_build_blueprint" remote="github/Adara" />
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed" remote="github/Adara" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Android Standart -->
  <project path="art" name="android_art" remote="github/Adara" />
  <project path="bionic" name="android_bionic" remote="github/Adara" />
  <project path="bootable/recovery" name="android_bootable_recovery" remote="github/Adara" />

  <!-- Android Vendor -->
  <project path="vendor/adara" name="android_vendor_adara" remote="github/Adara" />

</manifest>